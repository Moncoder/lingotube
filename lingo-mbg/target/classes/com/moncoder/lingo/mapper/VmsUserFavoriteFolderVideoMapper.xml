<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.moncoder.lingo.mapper.VmsUserFavoriteFolderVideoMapper">
    <select id="getBatchByIds" parameterType="java.util.List" resultType="com.moncoder.lingo.entity.VmsUserFavoriteFolderVideo">
        SELECT id,user_id,video_id,folder_id,create_time FROM vms_user_favorite_video
        WHERE id IN
        <foreach collection="list" item="item" open="(" close=")" separator=",">
            #{item}
        </foreach>
    </select>
</mapper>
